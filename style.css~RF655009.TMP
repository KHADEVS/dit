@import url('https://fonts.googleapis.com/css2?family=Roboto&display=swap');

body {
    font-family: 'Roboto', sans-serif;
    background-color : black;
    padding: 0;
    margin:0;
    color: white
}

body .page{
    margin:20px 80px;


}

/*Barre de navigation*/
nav{
    display: flex;
    flex-wrap : wrap;
    justify-content : space-between;
    position: sticky;
    top:0;
    background-color:black;
    padding:10px 0px;


}
/*fin de la barre */

nav .onglets{
    display: flex;
    flex-wrap:wrap;
    margin-top:5px ;
}

nav .onglets a{
    margin-right: 20px;
    cursor:pointer;

}
